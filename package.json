{
    "name": "@okto_web3/eliza-plugin",
    "version": "0.0.0",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "@elizaos/source": "./src/index.ts",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@elizaos/core": "0.2.0",
        "@okto_web3/core-js-sdk": "0.0.0",
        "axios": "^1.7.9",
        "ethers": "5.7.2",
        "google-auth-library": "^9.15.0",
        "open": "^10.1.0",
        "viem": "^2.22.2",
        "zod": "3.23.8"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "ts-node": "10.9.2",
        "tsup": "8.3.5",
        "typescript": "5.6.3",
        "vitest": "^1.0.0"
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "dev": "tsup --format esm --dts --watch",
        "lint": "eslint --fix  --cache .",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "agentConfig": {
        "pluginType": "elizaos:client:1.0.0",
        "pluginParameters": {
            "OKTO_ENVIRONMENT": {
                "type": "string",
                "description": "Okto environment (sandbox, staging, production)",
                "default": "sandbox"
            },
            "OKTO_CLIENT_PRIVATE_KEY": {
                "type": "string",
                "description": "Client private key provided by Okto",
                "required": true
            },
            "OKTO_CLIENT_SWA": {
                "type": "string", 
                "description": "Client SWA provided by Okto",
                "required": true
            },
            "GOOGLE_CLIENT_ID": {
                "type": "string",
                "description": "Google OAuth client ID from Google Cloud Console",
                "required": true
            },
            "GOOGLE_CLIENT_SECRET": {
                "type": "string",
                "description": "Google OAuth client secret from Google Cloud Console",
                "required": true
            }
        }
    }
}
